{
    "name": "@peacepieceuix-compatibility/themes",
    "version": "1.1.2",
    "author": "PrimeTek Informatics",
    "description": "",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/primefaces/primeuix.git",
        "directory": "packages/themes"
    },
    "bugs": {
        "url": "https://github.com/primefaces/primeuix/issues"
    },
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": "./src/index.ts",
        "./types": "./src/types/index.d.ts",
        "./types/*": "./src/types/*/index.d.ts",
        "./tokens": "./tokens.ts",
        "./*": "./src/presets/*/index.ts"
    },
    "publishConfig": {
        "main": "./index.mjs",
        "module": "./index.mjs",
        "types": "./index.d.mts",
        "exports": {
            ".": {
                "types": "./index.d.mts",
                "import": "./index.mjs"
            },
            "./types": {
                "types": "./types/index.d.ts"
            },
            "./types/*": {
                "types": "./types/*/index.d.ts"
            },
            "./tokens": {
                "types": "./tokens/index.d.ts",
                "import": "./tokens/index.mjs"
            },
            "./*": {
                "types": "./*/index.d.ts",
                "import": "./*/index.mjs"
            }
        },
        "directory": "dist",
        "linkDirectory": false,
        "access": "public"
    },
    "scripts": {
        "build": "cross-env NODE_ENV=production INPUT_DIR=src/ OUTPUT_DIR=dist/ pnpm run build:package",
        "build:package": "pnpm run build:prebuild && pnpm run typecheck && pnpm run build:tokens && tsup && pnpm run build:postbuild",
        "build:prebuild": "node ./scripts/prebuild.mjs",
        "build:postbuild": "node ./scripts/postbuild.mjs",
        "build:tokens": "node ./scripts/build-tokens.mjs",
        "typecheck": "tsc --noEmit",
        "dev:link": "pnpm link --global && npm link"
    },
    "dependencies": {
        "@peacepieceuix-compatibility/styled": "workspace:^"
    }
}
